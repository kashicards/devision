{
  "manifest_version": 3,
  "name": "Devision – Web & SEO Toolkit",
  "version": "1.0.2",
  "description": "Devision – The Ultimate Web & SEO Toolkit for Developers & Designers!",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo-16.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "cookies",
    "browsingData",
    "declarativeNetRequest",
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}