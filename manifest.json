{
  "manifest_version": 3,
  "name": "Devision",
  "version": "1.0",
  "description": "Devision â€“ The all-in-one toolkit for web optimization, SEO analysis & performance boost!",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo-16.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "cookies",
    "browsingData"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}